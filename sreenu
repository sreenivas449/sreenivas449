pipeline{
    agent any
    
    tools {
  maven 'maven'
}
    
    stages{
        stage('variables'){
            steps{
                echo "Hi Sreenivas"
                echo "${BUILD_NUMBER}"
                echo "${WORKSPACE}"
                echo "${JOB_URL}"
                echo "Node name is : ${NODE_NAME}"
                echo "build id is: ${BUILD_ID}"
                bat 'java -version'
                bat 'mvn -version'
            }
        }
    }
}
