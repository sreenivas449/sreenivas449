pipeline{
    agent any
    
    tools {
  jdk 'java'
  maven 'maven'
}
    
    stages{
        stage('variables'){
            steps{
                echo "Hi Sreenivas"
                echo "${BUILD_NUMBER}"
                echo "${WORKSPACE}"
                echo "${JOB_URL}"
                echo "Node name is : ${NODE_NAME}"
                echo "build id is: ${BUILD_ID}"
                java -version
                mvn -version
            }
        }
    }
}
